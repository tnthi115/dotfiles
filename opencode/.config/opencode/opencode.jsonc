    },
    "golangci-lint-langserver": {
      "command": ["/Users/t.thi/.local/share/nvim/mason/bin/golangci-lint-langserver"],
      "extensions": [".go"],
    },
    "taplo": {
      "command": ["/Users/t.thi/.local/share/nvim/mason/bin/taplo", "lsp", "stdio"],
      "extensions": [".toml"],
    },
  },
}
