    },
    "rust-analyzer": {
      "command": ["/Users/t.thi/.local/share/nvim/mason/bin/rust-analyzer"],
      "extensions": [".rs"],
    },
    "basedpyright": {
      "command": [
        "/Users/t.thi/.local/share/nvim/mason/bin/basedpyright-langserver",
        "--stdio",
      ],
      "extensions": [".py", ".pyi"],
    },
    "vtsls": {
      "command": ["/Users/t.thi/.local/share/nvim/mason/bin/vtsls", "--stdio"],
